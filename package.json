{
  "name": "mr--handlebars",
  "displayName": "Mr. Handlebars",
  "description": "Just another VScode extension",
  "version": "1.0.0",
  "repository": "https://github.com/CarlSaqui29/Mr.Handlebars",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "handlebars",
      "aliases": ["handlebars", "handlebars", "html-handlebars"],
      "extensions": [".html"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "html",
      "scopeName": "source.handlebars.grammar",
      "path": "./syntaxes/handlebars.tmLanguage.json"
    }]
  }
}
